<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.activities.DetalhesServicoBuscaActivity">

    <TextView
        android:id="@+id/nomeClinicaDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="114dp"
        android:layout_marginLeft="114dp"
        android:layout_marginTop="39dp"
        android:text="Nome clinica"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/localClinicaDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Local clinica"
        app:layout_constraintStart_toStartOf="@+id/nomeClinicaDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/nomeClinicaDetalheServico"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nomeDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="nome servico"
        app:layout_constraintStart_toStartOf="@+id/localClinicaDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/localClinicaDetalheServico"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/descricaoDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="descricao servico"
        app:layout_constraintStart_toStartOf="@+id/nomeDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/nomeDetalheServico"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/areaCorpoDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="area do corpo"
        app:layout_constraintStart_toStartOf="@+id/descricaoDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/descricaoDetalheServico"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/priceDetalheServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="preco"
        app:layout_constraintStart_toStartOf="@+id/areaCorpoDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/areaCorpoDetalheServico"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="11dp"
        android:text="Data"
        app:layout_constraintBottom_toBottomOf="@+id/textInputLayout8"
        app:layout_constraintEnd_toStartOf="@+id/textInputLayout8"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/textInputLayout8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/priceDetalheServico"
        tools:ignore="MissingConstraints">

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </android.support.design.widget.TextInputLayout>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="13dp"
        android:text="Horario"
        app:layout_constraintBottom_toBottomOf="@+id/textInputLayout9"
        app:layout_constraintEnd_toStartOf="@+id/textInputLayout9"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/textInputLayout9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout8"
        tools:ignore="MissingConstraints">

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </android.support.design.widget.TextInputLayout>

    <Button
        android:id="@+id/verificarDisponibilidadeServicoBusca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Verificar Disponibilidade"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/voltarDetalheServicoBusca"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout9"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/voltarDetalheServicoBusca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:text="Voltar"
        app:layout_constraintBottom_toTopOf="@+id/solicitarAgendamento"
        app:layout_constraintEnd_toStartOf="@+id/verificarDisponibilidadeServicoBusca"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/solicitarAgendamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:text="Solicitar Agendamento"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/descricaoDetalheServico"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="@+id/priceDetalheServico"
        app:layout_constraintTop_toBottomOf="@+id/verificarDisponibilidadeServicoBusca" />

    <TextView
        android:id="@+id/duracaoServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="duração"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="191dp" />
</android.support.constraint.ConstraintLayout>